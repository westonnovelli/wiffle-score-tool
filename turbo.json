{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "pipeline": {
        "build": {
          "dependsOn": ["^build"],
          "outputs": [".next/**"]
        },
        "test": {
          "dependsOn": ["build"],
          "outputs": [],
          "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
        },
        "typecheck": {
          "outputs": []
        },
        "deploy": {
          "dependsOn": ["build", "test", "lint"],
          "outputs": []
        }
      }
}
